{
    "extends": "airbnb",
    "rules": {
        "prefer-destructuring": [
            "error",
            {
                "object": true,
                "array": false
            }
        ],
        "func-style": 0,
        "arrow-parens": 0,
        "no-param-reassign": 0,
        "linebreak-style": 0
    },
    "env": {
        "browser": true
    }
}